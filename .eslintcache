           <Students />\n            </Route>\n            <Route exact path=\"/profile\">\n              <ProfilePage />\n            </Route>\n            <Route exact path=\"/activitiesregister\">\n              <ActivitiesRegister />\n            </Route>\n            <Route exact path=\"/activities\">\n              <Activities />\n            </Route>\n            <Route exact path=\"/activities/:id\">\n              <Activity />\n            </Route>\n          </>\n        ) : (\n          <Redirect to=\"/\" />\n        )}\n      </Switch>\n    </AnimatePresence>\n  );\n};\n\nexport default Routes;\n","/home/antonio/kkenzie-Q2/capstone/src/store/modules/activities/reducer.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/profile/reducer.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/login/reducer.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/students/reducer.js",[],"/home/antonio/kkenzie-Q2/capstone/src/components/Header/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/activities/actions-types.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/login/actions-types.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/profile/actions-types.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Home/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Login/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/User/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/Profile/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/students/actions-types.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Student/index.jsx",["210"],"import { useDispatch } from \"react-redux\";\nimport { registerStudentsThunk } from \"../../../store/modules/students/thunk\";\nimport * as yup from \"yup\";\nimport { yupResolver } from \"@hookform/resolvers/yup\";\nimport { useForm } from \"react-hook-form\";\nimport Alert from \"@material-ui/lab/Alert\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport {\n  Container,\n  Form,\n  Input,\n  TextArea,\n  ButtonContainer,\n  Button,\n  Title,\n  Error,\n} from \"./style\";\nimport { useHistory } from \"react-router-dom\";\nimport { useState } from \"react\";\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    width: \"50%\",\n    \"& > * + *\": {\n      marginTop: theme.spacing(2),\n    },\n  },\n}));\n\nconst StudentRegister = () => {\n  const classes = useStyles();\n  const [alertState, setAlertState] = useState(false);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const schema = yup.object().shape({\n    name: yup.string().required(\"Campo Obrigatório\"),\n    group: yup.string().required(\"Campo Obrigatório\"),\n    postscript: yup.string(),\n  });\n  const { register, handleSubmit, errors } = useForm({\n    resolver: yupResolver(schema),\n  });\n\n  const handleForm = (data, event) => {\n    dispatch(registerStudentsThunk(data));\n    setAlertState(true);\n    event.target.reset();\n  };\n  alertState && setTimeout(() => setAlertState(false), 3000);\n\n  return (\n    <Container>\n      <Form onSubmit={handleSubmit(handleForm)}>\n        {alertState && (\n          <Alert severity=\"success\">Aluno Cadastrado com Sucesso</Alert>\n        )}\n        <Title>Cadastrar Aluno</Title>\n        <Input name=\"name\" placeholder=\"Nome\" ref={register} />\n        {errors.name && <Error>{errors.name.message}</Error>}\n        <Input name=\"group\" placeholder=\"Turma\" ref={register} />\n        {errors.group && <Error>{errors.group.message}</Error>}\n        <TextArea name=\"postscript\" placeholder=\"Observações\" ref={register} />\n        {errors.postscript && <Error>{errors.postscript.message}</Error>}\n        <ButtonContainer>\n          <Button onClick={() => history.push(\"/students\")}>Voltar</Button>\n          <Button type=\"submit\">Cadastrar</Button>\n        </ButtonContainer>\n      </Form>\n    </Container>\n  );\n};\n\nexport default StudentRegister;\n","/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Activities/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Activity/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Activities/index.jsx",["211"],"import {useEffect, useState} from 'react';\nimport {useHistory} from 'react-router-dom';\nimport AlertFlag from '../../../components/AlertFlag';\nimport {\n  Container,\n  StyledLink,\n  Icon\n} from './style';\nimport {Button} from '../../Register/Activities/style';\nimport {useSelector, useDispatch} from  'react-redux';\nimport {listActivitiesThunk, deleteActivitiesThunk} from '../../../store/modules/activities/thunk';\nimport {ImBin2} from 'react-icons/im';\nimport { DataGrid} from \"@material-ui/data-grid\";\nimport { motion } from \"framer-motion\";\n\nconst Activities = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [alertState, setAlertState] = useState(false)\n  const activities = useSelector((state) => state.activities);\n  let rows = [];\n  let columns = [];\n\n  useEffect(() => {\n    dispatch(listActivitiesThunk());\n  }, [dispatch]);\n\n  const handleExclusion = (id) =>{\n    dispatch(deleteActivitiesThunk(id))\n    setAlertState(true)\n  }\n\n  activities.map((activity) =>{\n    rows= [...rows, {id: activity.id, col1: activity.name, col2: activity.date}] \n    columns = [\n      { field: 'col1', headerName: 'Atividade', width: 550 },\n      { field: 'col2', headerName: 'Data', width: 150 },\n      { field: 'col3', headerName: 'Detalhes', renderCell: () => <StyledLink to={`/activities/${activity.id}`}>+ detalhes</StyledLink>, width: 150 },\n      {field: 'col4', headerName: 'Excluir', width: 100, renderCell: () => <Icon onClick={() => handleExclusion(activity.id)}><ImBin2/></Icon>}\n    ]\n})\n\n  alertState && setTimeout(() => setAlertState(false), 3000)\n\n  return (\n    <motion.div\n      initial={{ opacity: 0 }}\n      animate={{ opacity: 1 }}\n      exit={{ opacity: 0 }}\n      transition={{ duration: 2 }}\n    >    \n      <Container>\n        {alertState && <AlertFlag severity=\"success\" text=\"Atividade excluida com sucesso\"/>}\n        <div style={{ height: 450, width: '100%' }}>\n          <DataGrid columns={columns} rows={rows}/>\n        </div> \n        <Button onClick={() => history.push(\"/activitiesregister\")}>\n          Cadastrar\n        </Button>\n      </Container>\n    </motion.div>\n  );\n};\n\nexport default Activities;\n","/home/antonio/kkenzie-Q2/capstone/src/store/modules/login/thunk.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/users/thunk.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Student/style.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/Profile/styled.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/styled.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/activities/thunk.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Activities/style.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/students/thunk.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Login/style.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/User/style.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/components/GetProfile/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/components/SearchById/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/components/GetStudents/index.jsx",["212"],"import { useEffect, useState } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport {\n  getStudentsThunk,\n  deleteStudentThunk,\n} from \"../../store/modules/students/thunk\";\nimport { DataGrid } from \"@material-ui/data-grid\";\nimport { ImBin2 } from \"react-icons/im\";\nimport { Box, Fix } from \"./styles\";\nimport { Icon } from \"../../pages/Lists/Activities/style\";\nimport AlertFlag from 