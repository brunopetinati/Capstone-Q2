ector((state) => state.students);\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  const schema = yup.object().shape({\n    name: yup.string().required(\"Campo Obrigatório\"),\n    date: yup.string().required(\"Campo Obrigatório\"),\n    description: yup.string(),\n    link: yup.string(),  \n  });\n  const { register, handleSubmit, errors } = useForm({\n    resolver: yupResolver(schema),\n  });\n\n  const registerActivity = (data, event) => {\n    dispatch(addActivityThunk(data));\n    setAlertState(true)\n    event.target.reset()\n  };\n\n  useEffect(() => {\n    dispatch(getStudentsThunk());\n  }, [dispatch]);\n  console.log(selected)\n  alertState && setTimeout(() => setAlertState(false), 3000)\n\n  \n\n  return (\n    <Container>\n      <Form onSubmit={handleSubmit(registerActivity)}>\n      {alertState && <Alert severity=\"success\">Atividade Cadastrada com Sucesso</Alert>}\n        <Title>Cadastrar</Title>\n        <MainInfo>\n          <Input name=\"name\" placeholder=\"Atividade\" ref={register} />\n          {errors.activity && <Error>{errors.activity.message}</Error>}\n          <Date name=\"date\" type=\"date\" ref={register} />\n          {errors.date && <Error>{errors.date.message}</Error>}\n        </MainInfo>        \n        \n        <TextArea name=\"description\" placeholder=\"Descrição\" />\n        {errors.description && <Error>{errors.description.message}</Error>}\n        <h3>Alunos</h3>\n            <List>\n              {students.map(({name, group}, index) =>{\n                return(\n                  <li key={index}><input ref={register} name=\"students\" type=\"checkbox\" value={`${name} - ${group}`} onChange={(e) => setSelected([...selected, e.target.value])}/>{name} - {group}</li>\n                )\n              })}\n            </List>\n        <Input name=\"link\" placeholder=\"Link da atividade\" style={{width: \"65vw\"}}/>\n        {errors.link && <Error>{errors.link.message}</Error>}\n        <ButtonContainer>\n          <Button onClick={() => history.push('/activities')}>Voltar</Button>\n          <Button type=\"submit\">Cadastrar</Button>\n        </ButtonContainer>\n      </Form>\n    </Container>\n  );\n};\n\nexport default ActivitiesRegister;\n","/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/Profile/styled.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/login/thunk.js",[],"/home/antonio/kkenzie-Q2/capstone/src/components/GetStudents/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/users/action.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/profile/actions.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Student/style.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/Activities/style.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Register/User/style.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/students/actions.js",[],"/home/antonio/kkenzie-Q2/capstone/src/store/modules/activities/actions-types.js",[],"/home/antonio/kkenzie-Q2/capstone/src/pages/Lists/Students/styled.js",[],"/home/antonio/kkenzie-Q2/capstone/src/components/GetProfile/index.jsx",[],"/home/antonio/kkenzie-Q2/capstone/src/components/Header/index.jsx",["80"],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":31,"column":9,"nodeType":"87","messageId":"88","endLine":31,"endColumn":16},{"ruleId":"85","severity":1,"message":"89","line":7,"column":9,"nodeType":"87","messageId":"88","endLine":7,"endColumn":19},{"ruleId":"90","severity":1,"message":"91","line":33,"column":6,"nodeType":"92","endLine":33,"endColumn":19,"suggestions":"93"},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-unused-vars","'classes' is assigned a value but never used.","Identifier","unusedVar","'makeStyles' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect 